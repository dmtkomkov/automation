{
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<tab> text ks=https://raw.githubusercontent.com/geerlingguy/packer-centos-7/master/http/ks.cfg<enter><wait>"
      ],
      
      "disk_size": 20000,
      "guest_os_type": "RedHat_64",
      "http_directory": "http",
      "headless": true,
      "iso_urls": [
        "CentOS-7-x86_64-Minimal-1511.iso",
        "http://mirror.yandex.ru/centos/7/isos/x86_64/CentOS-7-x86_64-Minimal-1611.iso"
      ],
      "iso_checksum_type": "sha256",
      "iso_checksum": "27bd866242ee058b7a5754e83d8ee8403e216b93d130d800852a96f41c34d86a",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "echo 'vagrant'|sudo -S /sbin/halt -h -p",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "virtualbox_version_file": ".vbox_version",
      "vm_name": "packer-centos-7-x86_64",
      "shutdown_command": "echo 'packer' | sudo -S shutdown -P now",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "2048"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ]
      ]
    }
  ],
  "post-processors": [
    {
      "output": "builds/{{.Provider}}-centos7.box",
      "type": "vagrant"
    }
  ]
}
